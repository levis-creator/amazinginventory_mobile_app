# Flutter App Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# API Configuration
# Production API URL
API_BASE_URL_PRODUCTION=https://amazinginventory.onrender.com/api/v1

# Development API URL (Laravel Herd)
# For local development: http://amazinginventory.test/api/v1
# For Android emulator: http://10.0.2.2:8000/api/v1
# For iOS simulator: http://localhost:8000/api/v1
API_BASE_URL_DEVELOPMENT=http://amazinginventory.test/api/v1

# Current environment (development or production)
# Set to 'development' for local development
# Set to 'production' for production builds
APP_ENV=development

# App Configuration
APP_NAME=Amazing Inventory
APP_VERSION=1.0.0

